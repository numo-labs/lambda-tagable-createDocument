{
  "name": "lambda-taggable-createdocument",
  "version": "2.0.0",
  "description": "create or update a taggable document",
  "main": "index.js",
  "scripts": {
    "nocov": "node ./node_modules/.bin/mocha test/*.js",
    "test": "semistandard && npm run coverage",
    "coverage": "node ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha test/*.js --report lcov -- -R spec",
    "lint": "node ./node_modules/eslint/bin/eslint.js --fix *.js **/*.js **/test/*.js",
    "deploy": "node ./node_modules/dpl/dpl.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/numo-labs/lambda-taggable-createDocument.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/numo-labs/lambda-taggable-createDocument/issues"
  },
  "homepage": "https://github.com/numo-labs/lambda-taggable-createDocument#readme",
  "dependencies": {
    "aws-lambda-helper": "^2.6.0",
    "aws4": "^1.3.2",
    "env2": "^2.0.7",
    "geo-hash": "0.0.2",
    "lodash": "^4.7.0",
    "request": "^2.69.0"
  },
  "devDependencies": {
    "aws-lambda-test-utils": "^1.0.2",
    "aws-sdk": "^2.2.40",
    "aws-sdk-mock": "^1.0.10",
    "decache": "^3.0.5",
    "dpl": "1.0.6",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "semistandard": "^7.0.5",
    "simple-mock": "^0.6.0"
  },
  "files_to_deploy": [
    "package.json",
    "index.js",
    "/lib",
    ".env"
  ],
  "semistandard": {
    "globals": [
      "describe",
      "it",
      "afterEach"
    ]
  },
  "pre-commit": [
    "semistandard",
    "check-coverage"
  ]
}
